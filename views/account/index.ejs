<div class="container">
  <% if (title) { %>
  <h1><%= title %></h1>
  <% } else { res.redirect('/') } %> <%- messages() %> <% if (errors) { %>
  <ul class="notice">
    <% errors.array().forEach(error => { %>
    <li><%= error.msg %></li>
    <% }) %>
  </ul>
  <% } %>

  <%# if (locals.accountData.account_type) { %>
  <h2>Welcome <%- locals.accountData.account_firstname %></h2>
  <%# } %>

  <p>You're logged in</p>
  <a href="/account/update">Update Account Information</a>

  <% if (locals.accountData.account_type) { %>
  <% if (locals.accountData.account_type != "Client") { %>
  <h3>Inventory Managment</h3>
  <p><a href="/inv">Inventory Managment</a></p>
  <% }} %>
</div>
