<div class="container">
  <% if (title) { %>
  <h1><%= title %></h1>
  <% } else { res.redirect('/') } %> <%- messages() %> <% if (errors) { %>
  <ul class="notice">
    <% errors.array().forEach(error => { %>
    <li><% error.msg %></li>
    <% }) %>
  </ul>
  <% } %>

  <form action="/inv/update" method="post">
    <input type="submit" name="submit" value="Update Vehicle" />
  </form>
</div>
